	if(str == null){
		return "";
	}else{
		str = str.replace(/\+/g," "); 
		str = str.replace(/@\(R\)@/g,"®"); 
		str = str.replace(/@\(-7\)@/g,"&");
		str = str.replace(/@\(-8\)@/g,"+"); 
		str = str.replace(/@\(-9\)@/g,"'"); 
		str = str.replace(/@\(TM\)@/g,"™"); 
